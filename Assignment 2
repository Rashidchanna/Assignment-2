#Problem 1

for i in range(1,11):
	print(i)
	
#Problem 2

num = int(input("Enter a number: "))

for i in range(1, 11):
    print(num, 'x', i, '=', num*i)
    
#Problem 3

num = int(input("Enter a number: "))
sum = 0
i = 1

while i <= num:
    sum += i
    i += 1

print("The sum of natural numbers up to", num, "is", sum)

#Problem 4

names = ['Khan', 'Ali', 'Arslan', 'Awais']

for name in names:
    print(name)

#Problem 5

num = int(input("Enter a number: "))
factorial = 1
i = 1

while i <= num:
    factorial *= i
    i += 1

print("The factorial of", num, "is", factorial)

#Problem 7

num = int(input("Enter a number: "))
reversed_num = 0

while num > 0:
    digit = num % 10
    reversed_num = reversed_num * 10 + digit
    num //= 10

print("The reversed number is:", reversed_num)

#Problem 8

string = input("Enter a string: ")
vowels = "aeiouAEIOU"
count = 0

for char in string:
    if char in vowels:
        count += 1

print("The number of vowels in the string is:", count)

#Problem 9

num = int(input("Enter a number: "))
temp = num
reverse_num = 0

while temp > 0:
    digit = temp % 10
    reverse_num = reverse_num * 10 + digit
    temp //= 10

if num == reverse_num:
    print(num, "is a palindrome number")
else:
    print(num, "is not a palindrome number")
    
 
#Problem 10

sum_of_squares = 0

for i in range(1, 6):
    sum_of_squares += i ** 2

print("The sum of the squares of the numbers from 1 to 5 is:", sum_of_squares)
